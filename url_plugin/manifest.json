{
	"name": "发布地址生成器",
	"description": "可以用来生成发布地址、短链接和二维码",
	"version": "1.1.130829",
	"manifest_version": 2,
	"browser_action": {
	"default_icon": "icon.png",
	"default_popup":"popup.html"
	},
	"content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
	"background":{
		"scripts" : ["popup.js"],
		"persistent": false
	},
	"permissions": [
		"tabs", "http://*/*", "https://*/*"
	]
}